{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: 'Product',\n  data() {\n    return {\n      product: {},\n      quantity: 1\n    };\n  },\n  mounted() {\n    this.getProduct();\n  },\n  methods: {\n    async getProduct() {\n      this.$store.commit('setIsLoading', true);\n      const category_slug = this.$route.params.category_slug;\n      const product_slug = this.$route.params.product_slug;\n      await axios.get(`api/v1/products/${category_slug}/${product_slug}`).then(response => {\n        this.product = response.data;\n        document.title = this.product.name + ' | Djackets';\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    },\n    addToCart() {\n      console.log('ADD CART');\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n      const item = {\n        product: this.product,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'The product was added to the cart',\n        type: 'is-success',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","toast","name","data","product","quantity","mounted","getProduct","methods","$store","commit","category_slug","$route","params","product_slug","get","then","response","document","title","catch","error","console","log","addToCart","isNaN","item","message","type","dismissible","pauseOnHover","duration","position"],"sources":["C:\\Users\\sergss\\Pictures\\TestSite\\djackets_vue\\src\\views\\Product.vue"],"sourcesContent":["<template>\r\n    <div class=\"page-product\">\r\n        <div class=\"columns is-multiline\">\r\n            <div class=\"column is-9\">\r\n                <figure class=\"image mb-6\">\r\n                    <img v-bind:src=\"product.get_image\">\r\n                </figure>\r\n\r\n                <h1 class=\"title\">{{ product.name }}</h1>\r\n\r\n                <p>{{ product.description }}</p>\r\n            </div>\r\n\r\n            <div class=\"column is-3\">\r\n                <h2 class=\"subtitle\">Information</h2>\r\n\r\n                <p><strong>Price: </strong>${{ product.price }}</p>\r\n\r\n                <div class=\"field has-addons mt-6\">\r\n                    <div class=\"control\">\r\n                        <input type=\"number\" class=\"input\" min=\"1\" v-model=\"quantity\">\r\n                    </div>\r\n\r\n                    <div class=\"control\">\r\n                        <a class=\"button is-dark\" @click=\"addToCart()\">Add to cart</a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\nimport { toast } from 'bulma-toast'\r\n\r\nexport default {\r\n    name: 'Product',\r\n    data() {\r\n        return {\r\n            product: {},\r\n            quantity: 1\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getProduct()\r\n    },\r\n    methods: {\r\n        async getProduct() {\r\n            this.$store.commit('setIsLoading', true)\r\n\r\n            const category_slug = this.$route.params.category_slug\r\n            const product_slug = this.$route.params.product_slug\r\n            \r\n            await axios\r\n                .get(`api/v1/products/${category_slug}/${product_slug}`)\r\n                .then(response => {\r\n                    this.product = response.data\r\n                    \r\n                    document.title = this.product.name + ' | Djackets'\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n\r\n            this.$store.commit('setIsLoading', false)\r\n        },\r\n        addToCart() {\r\n            console.log('ADD CART')\r\n            if (isNaN(this.quantity) || this.quantity < 1) {\r\n                this.quantity = 1\r\n            }\r\n\r\n            const item = {\r\n                product: this.product,\r\n                quantity: this.quantity\r\n            }\r\n\r\n            this.$store.commit('addToCart', item)\r\n\r\n            toast({\r\n                message: 'The product was added to the cart',\r\n                type: 'is-success',\r\n                dismissible: true,\r\n                pauseOnHover: true,\r\n                duration: 2000,\r\n                position: 'bottom-right',\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,KAAI,QAAS,aAAY;AAElC,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,CAAC,CAAC;MACXC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,UAAUA,CAAA,EAAG;MACf,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMC,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,aAAY;MACrD,MAAMG,YAAW,GAAI,IAAI,CAACF,MAAM,CAACC,MAAM,CAACC,YAAW;MAEnD,MAAMd,KAAI,CACLe,GAAG,CAAE,mBAAkBJ,aAAc,IAAGG,YAAa,EAAC,EACtDE,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACb,OAAM,GAAIa,QAAQ,CAACd,IAAG;QAE3Be,QAAQ,CAACC,KAAI,GAAI,IAAI,CAACf,OAAO,CAACF,IAAG,GAAI,aAAY;MACrD,CAAC,EACAkB,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK;MACrB,CAAC;MAEL,IAAI,CAACZ,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC5C,CAAC;IACDc,SAASA,CAAA,EAAG;MACRF,OAAO,CAACC,GAAG,CAAC,UAAU;MACtB,IAAIE,KAAK,CAAC,IAAI,CAACpB,QAAQ,KAAK,IAAI,CAACA,QAAO,GAAI,CAAC,EAAE;QAC3C,IAAI,CAACA,QAAO,GAAI;MACpB;MAEA,MAAMqB,IAAG,GAAI;QACTtB,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,QAAQ,EAAE,IAAI,CAACA;MACnB;MAEA,IAAI,CAACI,MAAM,CAACC,MAAM,CAAC,WAAW,EAAEgB,IAAI;MAEpCzB,KAAK,CAAC;QACF0B,OAAO,EAAE,mCAAmC;QAC5CC,IAAI,EAAE,YAAY;QAClBC,WAAW,EAAE,IAAI;QACjBC,YAAY,EAAE,IAAI;QAClBC,QAAQ,EAAE,IAAI;QACdC,QAAQ,EAAE;MACd,CAAC;IACL;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[